{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/modules/*": ["modules/*"],
      "@/components/*": ["components/*"],
      "@/layouts/*": ["layouts/*"],
      "@/styles/*": ["styles/*"],
      "@/lib/*": ["lib/*"],
      "@/router/*": ["router/*"],
      "@/i18n/*": ["i18n/*"],
      "@/types/*": ["types/*"]
    },
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/api/auth/[...nextauth].js", "pages/api/auth/[...nextauth].js", "next-auth.d.ts", "modules/community-project/templates/community/section/feed/components/room-list"],
  "exclude": ["node_modules"]
}
